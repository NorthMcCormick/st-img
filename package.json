{
  "name": "st-img",
  "version": "0.3.0",
  "description": "Web Component built with Stencil to lazy load images",
  "main": "dist/collection/index.js",
  "types": "dist/collection/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/stimg.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev"
  },
  "dependencies": {
    "@stencil/core": "0.0.8-9"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgw96/lazy-img.git"
  },
  "author": "Justin Willis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgw96/lazy-img"
  },
  "homepage": "https://github.com/jgw96/lazy-img"
}
